{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "prettier/prettier": ["error", { "endOfLine": "auto" }],
  "arrow-body-style": "off",
  "prefer-arrow-callback": "off"
}
